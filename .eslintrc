{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "commonjs": true
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "modules": true,
        "spread": true,
        "templateStrings": true,
        "jsx": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": [0],
        "key-spacing": [1],
        "quotes": [1, "single", "avoid-escape"],
        "quote-props": [1, "always"],
        "new-cap": [1],
        "no-multi-spaces": [0],
        "no-shadow": [1],
        "no-unused-vars": [1],
        "no-undef": 2,
        "no-unused-expressions": 0,
        "no-use-before-define": [2, "nofunc"],
        "no-trailing-spaces": [2],
        "eol-last": [0],
        "no-labels": [2],
        "indent": [1, "tab"],
        "vars-on-top": 0,
        "camelcase": [2, {"properties": "never"}],
        "semi": [1, "always"]
    }
}
